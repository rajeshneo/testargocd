  "metadata": {
    "name": "service2-stable",
    "namespace": "istio-test",
    "labels": {
      "app": "service2-stable"
    }
  },
  "spec": {
    "selector": {
      "app": "service2"
    },
    "type": "NodePort",
    "ports": [
      {
